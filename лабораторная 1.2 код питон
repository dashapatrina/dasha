#мультисписок

nested_list = [[1, 2], [3, 4]]
print(nested_list[0][1])  # выводит 2

#очереди
from collections import deque

queue = deque(['Alice', 'Bob'])
queue.append('Charlie')
print(queue.popleft())  # выведет Alice

#деки
deque_obj = deque([1, 2])
deque_obj.appendleft(0)
deque_obj.append(3)
print(deque_obj)  # [0, 1, 2, 3]
